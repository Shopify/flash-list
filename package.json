    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^2.0.0",
    "@types/react-native": "0.66.4",
    "babel-jest": "^26.6.3",
    "chalk": "^5.0.0",
    "detox": "^19.4.3",
    "eslint": "7.14.0",
    "glob": "^7.2.0",
    "jest": "^26.6.3",
    "metro-react-native-babel-preset": "^0.66.2",
    "react": "17.0.2",
    "react-native": "0.66.4",
    "react-native-flipper": "^0.129.0",
    "recyclerlistview": "3.1.0-beta.4",
    "typescript": "^4.1.3",
    "@shopify/eslint-plugin": "^41.0.1"
  },
